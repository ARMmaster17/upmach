<h1>Downtimes</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Machine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @downtimes.each do |downtime| %>
      <tr>
        <td><%= downtime.start_time %></td>
        <td><%= downtime.end_time %></td>
        <td><%= downtime.machine %></td>
        <td><%= link_to 'Show', machine_downtime_path(@machine, downtime) %></td>
        <td><%= link_to 'Edit', edit_machine_downtime_path(@machine, downtime) %></td>
        <td><%= link_to 'Destroy', [@machine, downtime], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>